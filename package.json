{
    "name": "@wolffs/cli",
    "version": "0.1.4",
    "description": "",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "tsup": {
        "entry": [
            "src/index.ts"
        ],
        "format": [
            "cjs",
            "esm"
        ],
        "splitting": false,
        "sourcemap": true,
        "clean": true
    },
    "files": [
        "./dist"
    ],
    "bin": {
        "wolff-gen": "./dist/index.js"
    },
    "scripts": {
        "start": "ts-node src/index.ts",
        "typecheck": "tsc --noEmit",
        "build": "tsup && shx rm -rf dist/templates && shx cp -r src/templates dist",
        "prepack": "npm run build",
        "release": "release-it"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/ejs": "^3.1.2",
        "@types/inquirer": "^9.0.3",
        "ejs": "^3.1.9",
        "nodemon": "^3.0.1",
        "release-it": "^16.1.3",
        "shx": "^0.3.4",
        "ts-node": "^10.9.1",
        "tsup": "^7.2.0",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "inquirer": "^8.2.6"
    }
}
